


<% for(var i = 0; i < contact.length; i++) { %>
    <tr>
      <td><%= i + 1 %></td>
      <td><%= contact[i].name %></td>
      <td><%= contact[i].email %></td>
      <td><%= contact[i].phone %></td>
      <td>
        <a href="/dashboard/edit/<%= contact[i]._id %>" class="btn btn-primary btn-sm">
        <i class="fas fa-pencil-alt"></i></a>
    </td>
    <td>
        <a href="/dashboard/delete/<%= contact[i]._id %>" class="btn btn-danger btn-sm">
        <i class="fas fa-trash-alt"></i></a>
    </td>
    </tr>
<% } %>